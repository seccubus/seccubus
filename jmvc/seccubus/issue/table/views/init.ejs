<tr>
	<td>Name</td>
	<td>Description</td>
	<td>External reference</td>
	<td>Severity</td>
	<td># of findings</td>
	<td>Status</td>
</tr>
<% if ( this.length == 0 ) { %>
	<%== $.View('//seccubus/issue/table/views/error.ejs') %>
<% } else { %>
	<option selected value=-1 disabled=true>Select a issue below</option>
	<% for(var i = 0; i < this.length ; i++) { %>
		<option <%= this[i] %> value='<%= this[i].id %>'>
			<%== $.View('//seccubus/issue/table/views/issue.ejs', this[i] )%>
		</option>
	<% } %>
<% } %>
